{
  "name": "@figma/my-make-file",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "verify": "node verify-setup.js",
    "verify:workflow": "node verify-workflow.js",
    "emergency-fix": "node emergency-fix.cjs",
    "fix-imports": "node fix-all-imports.cjs",
    "diagnostic": "node diagnostic-check.cjs",
    "wizard": "node build-apk-wizard.js",
    "fix-crash": "rm -rf android && npm run build && npx cap add android && npx cap sync android",
    "cap:init": "npx cap init 'NEWS ROBO' 'com.newsrobo.app'",
    "cap:add": "npx cap add android",
    "cap:sync": "npm run build && npx cap sync android",
    "cap:open": "npx cap open android",
    "cap:run": "npx cap run android",
    "android:setup": "npm run cap:init && npm run cap:add",
    "android:build": "npm run build && npx cap sync android",
    "android:open": "npm run android:build && npx cap open android",
    "android:dev": "npx cap run android --livereload --external",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:doctor": "npx cap doctor",
    "netlify:dev": "netlify dev",
    "netlify:build": "npm run build",
    "netlify:deploy": "npm run build && netlify deploy --prod",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "firebase:deploy": "npm run build && firebase deploy",
    "firebase:serve": "npm run build && firebase serve",
    "deploy:all": "npm run build && npm run netlify:deploy && npm run firebase:deploy",
    "workflow:dev": "npm run dev",
    "workflow:build": "npm run build",
    "workflow:android": "npm run android:open",
    "workflow:web": "npm run netlify:deploy",
    "workflow:complete": "npm run build && npx cap sync android && npm run netlify:deploy"
  },
